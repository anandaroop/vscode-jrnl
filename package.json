{
  "name": "jrnl",
  "displayName": "Jrnl",
  "description": "Highlighting for the Jrnl text file format",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.34.0"
  },
  "license": "MIT",
  "preview": true,
  "publisher": "anandaroop",
  "repository": "https://github.com/anandaroop/vscode-jrnl",
  "categories": [
    "Other"
  ],
  "contributes": {
    "languages": [
      {
        "id": "jrnl",
        "aliases": [
          "Jrnl",
          "jrnl"
        ],
        "extensions": [
          "jrnl.txt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "jrnl",
        "scopeName": "text.jrnl",
        "path": "./syntaxes/jrnl.tmLanguage.json"
      }
    ]
  }
}
